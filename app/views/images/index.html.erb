      <div id="gallery">
        <h2>All Images</h2>
        <% if @images.exists? %>
        	<% @images.each_slice(4) do |image| %>
        	<div class="row"> 
        		<% image.each do |image| %>
        			<div class = 'col-md-3'>
			            <div class = "photo photo-type-line">
			              <a data-fancybox = "my-images" href = "<%= image.image_url() %>" class = "photo-hover" data-caption = " <%= image.image_title %>">
			                <div class = "photo-info">
			                  <div class = "headline"> <%= image.image_title %> </div>
			                  <div class = "line"> </div>
			                  <div class = "desc"> <%= image.image_description %> </div>
			                </div>
			                <div class = "mask"> </div>
			              </a>
			              <div class = "photo-img"> <%= image_tag image.image_url(:thumb) %> </div>
			          </div>
			      </div>
        	<% end %>
        </div>
    <% end %>
    <% else %>
    <p> There are no images</p>
<% end %>
</div>
